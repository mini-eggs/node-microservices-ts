{
  "name": "node-microservices-ts",
  "version": "0.2.0",
  "main": "dist/index.js",
  "dependencies": {
    "express": "^4.15.2"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.15",
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "nodemon": "^1.11.0",
    "pkg": "^3.0.1",
    "typescript": "^2.3.2"
  },
  "scripts": {
    "build": "rm -rf pkg && tsc -p tsconfig.json && pkg dist/index.js --output pkg/server && rm -rf dist",
    "start": "./pkg/server",
    "dev": "concurrently --kill-others \"tsc -w -p tsconfig.json\" \"nodemon dist/\""
  }
}